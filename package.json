  "bugs": {
    "url": "https://github.com/dstructs/typed-array-to-json/issues"
  },
  "dependencies": {
    "dstructs-cast-arrays": "^1.0.2",
    "type-name": "^1.1.0",
    "validate.io-typed-array": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "codecov.io": "^0.1.5",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
    "mocha": "2.x.x"
  },
  "license": "MIT"
